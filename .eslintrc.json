{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console": "warn",
    "react-hooks/rules-of-hooks": 2,
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["react", "import", "jsx-a11y", "react-hooks"],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
